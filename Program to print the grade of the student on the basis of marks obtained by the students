#include<stdio.h>
int main(){
    int x;
    printf("Enter total marks obtained: ");
    scanf("%d", &x);
    if(x>=90)
        printf("You got A+");
    else if(x>=80)
        printf("You got A");
    else if(x>=70)
        printf("You got B+");
    else if(x>=60)
        printf("You got B");
    else if(x>=50)
        printf("You got C");
    else
        printf("You got F");
        return 0;
}
