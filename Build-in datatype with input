#include<stdio.h>

int main(){
    char ch;
    int x;
    float y;
    double z;
    printf("enter a CHAR ");
    scanf("%c", &ch);
    printf("Enter a number ");
    scanf("%d", &x);
    printf("Enter a float value ");
    scanf("%f", &y);
    printf("Enter a double type value ");
    scanf("%lf", &z);
    printf("the value of ch is %c \nValue of x is %d \nValue of y is %f \nValue of z is %lf",ch, x, y, z );

    return 0;
}
