#include<stdio.h>

int main(){
    float x1, x2, x3, x4, x5;
    printf("Enter the marks of subject 1 ");
    scanf("%f", &x1);
    printf("Enter the marks of subject 2 ");
    scanf("%f", &x2);
    printf("Enter the marks of subject 3 ");
    scanf("%f", &x3);
    printf("Enter the marks of subject 4 ");
    scanf("%f", &x4);
    printf("Enter the marks of subject 5 ");
    scanf("%f", &x5);
    printf("The total marks student obtain is %.2f \nThe percentage of the student is %.2f percent", x1+x2+x3+x4+x5, ((x1+x2+x3+x4+x5)/500)*100);
    return 0;
}
