#include<stdio.h>
#include<math.h>

int main(){
    float x1,x2,y1,y2,d;
   printf("Enter X and Y axis of point 1 ");
   scanf("%f%f",&x1,&y1);
   printf("Enter x and y axis of point 2 ");
   scanf("%f%f",&x2,&y2);
   d= sqrt(pow(x2-x1, 2)+pow(y2-y1, 2));
   printf("The distance between X and Y is %f", d);
   return 0;
}
